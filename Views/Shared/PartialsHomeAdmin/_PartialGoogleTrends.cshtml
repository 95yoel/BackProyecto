<script type="text/javascript" src="https://ssl.gstatic.com/trends_nrtr/3349_RC01/embed_loader.js"></script>
<script type="text/javascript">
    var sitio1 = '@Html.Raw(ViewBag.site1)';
    var sitio2 = '@Html.Raw(ViewBag.site2)';
    var tiempo = '@Html.Raw(ViewBag.timeRange)';

    trends.embed.renderExploreWidget("GEO_MAP", {
        "comparisonItem": [
            {
                "keyword": sitio1,
                "geo": "ES",
                "time": tiempo
            },
            {
                "keyword": sitio2,
                "geo": "ES",
                "time": tiempo
            }
        ],
        "category": 0,
        "property": ""
    }, {
        "exploreQuery": "date=" + tiempo + "&geo=ES&q=" + sitio1 + "," + sitio2 + "&hl=es",
        "guestPath": "https://trends.google.es:443/trends/embed/"
    });
</script>
